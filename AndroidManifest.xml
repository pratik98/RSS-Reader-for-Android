<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.pratik.android.rss"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="9" />
	<uses-permission android:name="android.permission.INTERNET" />
    <application android:icon="@drawable/nr" android:label="@string/app_name">
        <activity android:name=".Start"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".categories"
                  android:label="@string/app_name">
           <intent-filter>
                <action android:name="com.pratik.android.rss.CLEARSCREEN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".AndroidRSSReader" android:label="@string/app_name">
					</activity>

		<activity android:name=".RssItemDisplayer" android:label="Display Rss Item">
			<intent-filter>
				<action android:name="com.pratik.android.rss.displayRssItem" />
				<category android:name="android.intent.category.DEFAULT"></category>
			</intent-filter>
		</activity>
		<activity android:name=".AddCategory"
                  android:label="@string/app_name" />
        <activity android:name=".News"
                  android:label="@string/app_name" />
        <activity android:name=".quicknews"
                  android:label="@string/app_name" />
        <activity android:name=".ViewSavedCats"
                  android:label="@string/app_name" />
    </application>
    <application android:name="Myapplication" android:icon="@drawable/icon" android:label="@string/app_name" >
    </application>
    
</manifest>